## Python for Brownian motion

T = 1     # end of interval[0,T]
N = 200
x=0       #initial value of the process at time t0.
t0=0      #initial time
dt = (T-t0)/N

## R 原檔
t <- seq(t0,T, length=N+1)  # set up scale
X1 <- ts(cumsum(c(x,rnorm(N,mean = 0)*sqrt(dt))),start=t0, deltat=dt)

ts.plot(X1)
mean(X1)
sd(X1)